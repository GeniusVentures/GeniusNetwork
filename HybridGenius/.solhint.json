{
    "extends": "solhint:default",
    "rules": {
      "indent": ["error", 4],
      "func-order": false,
      "bracket-align": false,
      "compiler-fixed": false,
      "no-simple-event-func-name": false,
      "separate-by-one-line-in-contract": false,
      "two-lines-top-level-separator": false
    }
}